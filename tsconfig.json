{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "20.1.0",
  "compilerOptions": {
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2022",
    "sourceMap": true,
    "outDir": "./dist/src",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "lib": ["es2023"],
    "module": "node16",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node16"
  }
}